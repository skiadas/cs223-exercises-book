    <exercise xml:id="exer-JMPCH10_code-array5-1" label="exe-sec-Homework-4-Arrays-Strings-1-234208">
      <statement>
        <p>Write a function <c>count_occurrences</c> that takes an array of integers 'arr', an integer 'size', and an integer 'n'. The function returns an integer indicating the number of occurrences of the value 'n' among the first size-many elements in 'arr'.</p>
        <p>For example, if arr = [3, 3, 5, 3, 3, 2, 8], then <c>count_occurrences(arr, 3, 5)</c> returns 1 since there is one '5' in [3, 3, 5], and count_occurrences(arr, 6, 3) returns 4 since there are four 3's in [3, 3, 5, 3, 3, 2].</p>
      </statement>
      <program interactive="activecode" language="cpp">
        <preamble visible="no">#include &lt;sstream&gt;


        </preamble>
        <code>

        </code>
        <postamble visible="no" />
        <tests>
#define DOCTEST_CONFIG_IMPLEMENT_WITH_MAIN
#include "doctest.h"


TEST_CASE( "test1" ) {
    std::stringstream cout;
    int arr[10] = {0, 0, 1, 1, 2, 2, 3, 3, 4, 4};
    // size = 4, n = 1, should return 2
    cout &lt;&lt; count_occurrences(arr, 4, 1);

    ;
    std::string answer = "2";
    CHECK( cout.str() == answer );
}


TEST_CASE( "test2" ) {
    std::stringstream cout;
    int arr[10] = {0, 0, 1, 1, 2, 2, 3, 3, 4, 4};
    // size = 4, n = -1, should return 0
    cout &lt;&lt; count_occurrences(arr, 4, -1);

    ;
    std::string answer = "0";
    CHECK( cout.str() == answer );
}


TEST_CASE( "test3" ) {
    std::stringstream cout;
    int arr[10];
    for(int i=0; i&lt;10; i++){arr[i] = 7;}
    // arr = [7, 7, ..., 7]
    // size = 4, n = 7, should return 4
    cout &lt;&lt; count_occurrences(arr, 4, 7);

    ;
    std::string answer = "4";
    CHECK( cout.str() == answer );
}


TEST_CASE( "test4" ) {
    std::stringstream cout;
    int arr[10];
    for(int i=0; i&lt;10; i++){arr[i] = 7;}
    // arr = [7, 7, ..., 7]
    // size = 10, n = 7, should return 10
    cout &lt;&lt; count_occurrences(arr, 10, 7);

    ;
    std::string answer = "10";
    CHECK( cout.str() == answer );
}


TEST_CASE( "test5" ) {
    std::stringstream cout;
    int arr[100];
    for(int i=0; i&lt;100; i++){arr[i] = 7*(i%2);}
    // arr = [0, 7, 0, 7, ..., 0, 7]
    // size = 3, n = 7, should return 1
    cout &lt;&lt; count_occurrences(arr, 3, 7);

