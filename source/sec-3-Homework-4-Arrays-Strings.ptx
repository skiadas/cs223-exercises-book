    for(int i=0; i&lt;20; i++){arr[i] = i;}
    arr[3] = arr[17];
    // arr = [0, 1, 2, 17, 4, 5, ...., 16, 17, 18, 19], has repeat
    cout &lt;&lt; unique_elements(arr, 20);

    ;
    std::string answer = "0";
    CHECK( cout.str() == answer );
}

