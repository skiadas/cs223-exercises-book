    <exercise xml:id="exer-M-S_CH10_short-tree_terminology4-1" label="exe-sec-Homework-14-Binary-Trees-1-234433">
      <statement>
        <p>
          What are the child node(s) of "ontario"? If there is more than one child, list them with a single space in between.
          <image decorative="yes" source="images/234433-example_tree.png" width="90%" />
        </p>
        <fillin answer="winnipeg vostok" width="16" />
      </statement>
      <evaluation>
        <evaluate>
          <test correct="yes">
            <strcmp>winnipeg vostok</strcmp>
            <feedback />
          </test>
          <test>
            <strcmp>vostok winnipeg</strcmp>
            <feedback />
          </test>
          <test>
            <strcmp>winnipeg, vostok</strcmp>
            <feedback />
          </test>
          <test>
            <strcmp>vostok, winnipeg</strcmp>
            <feedback />
          </test>
        </evaluate>
      </evaluation>
    </exercise>
    <exercise xml:id="exer-M-S_CH10_short-tree_terminology6-1" label="exe-sec-Homework-14-Binary-Trees-1-234435">
      <statement>
        <p>What is the size of the subtree rooted at "huron"?</p>
        <p>
          <image decorative="yes" source="images/234435-example_tree.png" width="90%" />
        </p>
        <fillin answer="6" width="16" />
